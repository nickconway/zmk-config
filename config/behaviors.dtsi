hrA: hrA {
    compatible = "zmk,behavior-hold-tap";
    label = "HOME_ROW_A";
    #binding-cells = <2>;
    flavor = "balanced";
    tapping-term-ms = <TAPPING_TERM_PINKY>;
    quick-tap-ms = <QUICK_TAP_TERM>;
