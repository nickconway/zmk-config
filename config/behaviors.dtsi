bht: balanced_hold_tap {
    compatible = "zmk,behavior-hold-tap";
    label = "BALANCED_HOLD_TAP";
    #binding-cells = <2>;
    flavor = "balanced";
    tapping-term-ms = <200>;
    quick-tap-ms = <200>;
    bindings = <&kp>, <&kp>;
};

altq: altq {

    compatible = "zmk,behavior-mod-morph";
    label = "ALT_Q";
    #binding-cells = <0>;
    bindings = <&ASMQ>, <&kp F4>;
    mods = <(MOD_LALT)>;

};

wing: wing {

    compatible = "zmk,behavior-mod-morph";
    label = "WIN_GRAVE";
    #binding-cells = <0>;
    bindings = <&altq>, <&kp GRAVE>;
    mods = <(MOD_LGUI)>;

};

